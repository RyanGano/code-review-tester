# Microsoft Entra ID Configuration
# Copy this file to .env.local and fill in your actual values
# NEVER commit the .env.local file to version control!

# Required: Your Microsoft Entra ID Tenant ID
VITE_AZURE_TENANT_ID=YOUR_TENANT_ID

# Required: Your Microsoft Entra ID Application (Client) ID
VITE_AZURE_CLIENT_ID=YOUR_CLIENT_ID

# Optional: Custom redirect URI (defaults to current origin)
VITE_REDIRECT_URI=http://localhost:5173

# Optional: Custom post-logout redirect URI (defaults to current origin)
VITE_POST_LOGOUT_REDIRECT_URI=http://localhost:5173

# Optional: API base URL (defaults to http://localhost:5137)
VITE_API_BASE_URL=http://localhost:5137