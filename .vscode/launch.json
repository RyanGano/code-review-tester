{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Debug code-review-trainer-service",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/code-review-trainer-service/bin/Debug/net8.0/code-review-trainer-service.dll",
      "args": [],
      "cwd": "${workspaceFolder}/code-review-trainer-service",
      "stopAtEntry": false,
      "console": "internalConsole",
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      }
    }
  ]
}
